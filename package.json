{
  "name": "copy-paste-component-vscode",
  "displayName": "copy-paste-component-vscode",
  "description": "A tool to create components, by copying and pasting existing ones.",
  "version": "0.0.1",
  "publisher": "GabrielDuarteM",
  "license": "MIT",
  "engines": {
    "vscode": "^1.19.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.cpc"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.cpc",
        "title": "Copy and paste component"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.cpc",
          "group": "navigation@1"
        }
      ]
    }
  },
  "scripts": {
    "commit": "git-cz",
    "vscode:prepublish": "yarn compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "yarn compile && node ./node_modules/vscode/bin/test"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.6"
  },
  "dependencies": {
    "copy-paste-component": "^1.2.0",
    "prettier": "^1.10.2"
  }
}
